{
	"name": "react-intl-provider",
	"version": "0.0.4",
	"description": "Simple React provider that enables language configuration of intl object",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "tslint -p tsconfig.json",
		"prepare": "npm run build",
		"preversion": "npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alchemisten/react-intl-provider.git"
	},
	"keywords": [
		"react",
		"library",
		"typescript",
		"ts"
	],
	"author": "Alchemisten AG",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/alchemisten/react-intl-provider/issues"
	},
	"homepage": "https://github.com/alchemisten/react-intl-provider#readme",
	"devDependencies": {
		"@types/lodash": "^4.14.165",
		"@types/react": "^16.13.1",
		"prettier": "^1.13.2",
		"tslint": "^5.10.0",
		"tslint-config-prettier": "^1.13.0",
		"typescript": "~4.0.5",
		"react": "^16.13.1",
		"react-intl": "^5.10.4"
	},
	"dependencies": {
		"lodash": "^4.17.20"
	},
	"peerDependencies": {
		"react-intl": "^5.10.4"
	}
